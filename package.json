{
  "name": "lofi-doro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prettier-lint": "prettier --write .",
    "cy-chrome": "cypress run --browser chrome",
    "cy-firefox": "cypress run --browser firefox",
    "cypress:open": "cypress open",
    "export": "next build && next export",
    "predeploy": "yarn export",
    "deploy": "gh-pages -d out"
  },
  "husky": {
    "hooks": {
      "applypatch-msg": "[Husky] patch",
      "pre-applypatch": "[Husky] starting patch",
      "post-applypatch": "[Husky] patch done",
      "pre-commit": "prettier-lint"
    }
  },
  "lint-staged": {
    "*": "prettier --write"
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.5.5",
    "@react-spring/web": "^9.4.3",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-youtube": "^7.14.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "cypress": "^9.5.1",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  }
}
